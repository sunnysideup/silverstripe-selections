{
    "name": "sunnysideup/selections",
    "description": "Allows the CMS user to access the ORM to create record selections",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "ORM",
        "selection",
        "filter",
        "filtering",
        "sorting",
        "display",
        "gridfield",
        "cms helpers",
        "powerfull",
        "lists",
        "save filters"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2.0"
    },
    "require-dev": {
        "sunnysideup/sswebpack_engine_only": "dev-master",
        "silverleague/ideannotator": "*",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7",
        "silverstripe/standards": "^1",
        "phpstan/extension-installer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Sunnysideup\\Selections\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "bin/sake-npm-build-selections.sh",
        "bin/sake-npm-watch-selections.sh"
    ],
    "authors": [
        {
            "name": "Nicolaas Thiemen Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "lead developer"
        }
    ]
}